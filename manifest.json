{
  "manifest_version": 2,
  "name": "Get Engine Edition Extension",
  "description": "Get the facebook instant ganme Engine Edition",
  "version": "1.0",
  "permissions": [
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.apps.fbsbx.com/instant-bundle/*"
      ],
      "js": [
        "js/contentscript.js"
      ],
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Get Engine"
  }
}